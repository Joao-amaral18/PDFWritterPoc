<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="..\.dockerignore">
      <Link>.dockerignore</Link>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="itext7" Version="8.0.1"/>
    <PackageReference Include="itext7.bouncy-castle-adapter" Version="8.0.1"/>
    <PackageReference Include="itext7.bouncy-castle-fips-adapter" Version="8.0.1"/>
    <PackageReference Include="itext7.pdfhtml" Version="5.0.1"/>
    <PackageReference Include="PDFtoImage" Version="2.3.2"/>
    <PackageReference Include="BitMiracle.Docotic.Pdf" Version="9.1.15876"/>
  </ItemGroup>
</Project>